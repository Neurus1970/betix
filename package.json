{
  "name": "betix",
  "version": "1.0.0",
  "description": "Plataforma de estad√≠sticas de tickets por provincia y juego",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest --coverage --verbose",
    "test:ci": "jest --coverage --ci --reporters=default --reporters=jest-junit",
    "lint": "eslint src/ tests/"
  },
  "keywords": ["betix", "estadisticas", "tickets"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "supertest": "^6.3.4",
    "eslint": "^8.57.0",
    "nodemon": "^3.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": ["text", "lcov", "clover"],
    "reporters": [
      "default",
      ["jest-junit", {
        "outputDirectory": "test-results",
        "outputName": "junit.xml"
      }]
    ]
  }
}
